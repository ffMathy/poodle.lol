{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "output",
  "builds": [
    {
      "src": "output/backend/index.mjs",
      "use": "@vercel/node"
    }, 
    {
      "src": "output/static/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "output/api/index.mjs"
    },
    {
      "src": "/(.*)",
      "dest": "output/static/entry.vercel-edge.js"
    }
  ]
}