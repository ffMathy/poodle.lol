{
  "name": "frontend",
  "description": "App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "packageManager": "pnpm@7.15.0",
  "private": true,
  "scripts": {
    "vercel-build": "npm run build",
    "build": "npm run build.frontend && npm run build.backend",
    "build.frontend": "vite build -c vite.config.frontend.ts",
    "build.preview": "vite build --ssr src/frontend/entry.preview.tsx -c vite.config.frontend.ts",
    "build.backend": "vite build -c vite.config.backend.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "vercel deploy",
    "dev": "vite --mode ssr -c vite.config.frontend.ts",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force -c vite.config.frontend.ts",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr -c vite.config.frontend.ts"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.29.5",
    "@trpc/client": "10.21.2",
    "@trpc/express": "10.21.2",
    "@trpc/next": "10.21.2",
    "@trpc/react-query": "10.21.2",
    "@trpc/server": "10.21.2",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.2",
    "superjson": "^1.12.3",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.0.0",
    "@builder.io/qwik-city": "^1.0.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/aws-lambda": "^8.10.114",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.16.2",
    "@types/uuid": "^9.0.1",
    "autoprefixer": "^10.4.13",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.16",
    "prettier": "2.8.6",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "5.0.2",
    "undici": "5.21.0",
    "vercel": "^28.4.17",
    "vite": "4.2.1",
    "vite-tsconfig-paths": "3.5.0"
  }
}
